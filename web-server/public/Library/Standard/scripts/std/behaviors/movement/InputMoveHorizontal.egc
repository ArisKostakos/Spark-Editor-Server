<?xml version="1.0" encoding="utf-8"?>
<Entity>
	<_Actions>
		<Action>
			<Id>GoLeft</Id>
			<Concurrency>Persistent</Concurrency>
			<Scripts>
				<Script>
					me.setState('spaceX',me.getState('spaceX')-3);
				</Script>
			</Scripts>
		</Action>
		
		<Action>
			<Id>GoRight</Id>
			<Concurrency>Persistent</Concurrency>
			<Scripts>
				<Script>
					me.setState('spaceX',me.getState('spaceX')+3);
				</Script>
			</Scripts>
		</Action>
	</_Actions>
</Entity>