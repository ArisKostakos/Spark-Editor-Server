<?xml version="1.0" encoding="utf-8"?>
<Entity>
	<_States>
		<_State id="physicsSensorFlag">
			<Value>true</Value>
		</_State>
		<_State id="physicsCbType">
			<Value>Collectible</Value>
		</_State>
	</_States>
	
	<_Triggers>
		<Trigger>
			<Event>PhysicsSensorStart</Event>
			<Scripts>
				<Script>
					Display.error(me.getState('name') + " COLLECTED!");
					parent.removeChild(me);
				</Script>
			</Scripts>
		</Trigger>
	</_Triggers>
</Entity>