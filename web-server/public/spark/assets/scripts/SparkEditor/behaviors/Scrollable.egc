<?xml version="1.0" encoding="utf-8"?>
<Entity>
	<_States>
		<_State id="touchable"><Value>true</Value></_State>
		<_State id="top"><Value>0</Value></_State>
	</_States>
		
	<_Triggers>
		<Trigger>
			<Event>MouseScroll</Event>
			<Scripts>
				<Script>
					var myTop = Math.min(0,me.getState('top')-(me.getState('eventObject').deltaY));
					me.setState('top',myTop);
				</Script>
			</Scripts>
		</Trigger>
	</_Triggers>
</Entity>