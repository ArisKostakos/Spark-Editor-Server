		<Entity extends="SparkEditor.levels.projectEditor.tabs.levelEditor.gadgets.RotateGadgetHandle">
			<_States>
				<_State id="top"><Value>40</Value></_State>
				<_State id="left"><Value>-3</Value></_State>
			</_States>
		</Entity>
	</Entities></Space></Form>
