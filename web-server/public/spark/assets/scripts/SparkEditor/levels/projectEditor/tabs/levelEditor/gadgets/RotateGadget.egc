			</_Scripts>
		</_Action>
	</_Actions>
		
	<Form><Space><Entities>
		<!-- Cross Horizontal -->
		<Entity extends="std.display.Div">
			<_States>
				<_State id="top"><Value>0</Value></_State>
				<_State id="left"><Value>-4</Value></_State>
				<_State id="width"><Value>11</Value></_State>
				<_State id="height"><Value>3</Value></_State>
				<_State id="pointerEvents"><Value>none</Value></_State>
				<_State id="backgroundColor"><Value>rgb(30,38,45)</Value></_State>
			</_States>
		</Entity>
		<!-- Cross Vertical -->
		<Entity extends="std.display.Div">
			<_States>
				<_State id="top"><Value>-4</Value></_State>
				<_State id="left"><Value>0</Value></_State>
				<_State id="width"><Value>3</Value></_State>
				<_State id="height"><Value>11</Value></_State>
				<_State id="pointerEvents"><Value>none</Value></_State>
				<_State id="backgroundColor"><Value>rgb(30,38,45)</Value></_State>
			</_States>
		</Entity>
		
		<!-- Line -->
		<Entity extends="std.display.Div">
			<_States>
				<_State id="top"><Value>0</Value></_State>
				<_State id="left"><Value>1</Value></_State>
				<_State id="width"><Value>1</Value></_State>
